{
    "name": "logbucket",
    "author": "DJj123dj",
    "version": "1.1.0",
    "description": "A small package to create awesome and beautiful logs in the node.js console with colors, prefixes and the ability to be saved in a debug file.",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "scripts": {
        "build": "node ./tools/cleanup.js && tsc -p ./tsconfig.json",
        "example:simple": "node ./examples/simpleExample.js",
        "example:complex": "node ./examples/complexExample.js",
        "example:module": "node ./examples/moduleExample.mjs",
        "example:event": "node ./examples/eventExample.js"
    },
    "keywords": [
        "color",
        "console",
        "ansis",
        "simple",
        "logs",
        "log",
        "debug",
        "rgb",
        "chalk",
        "logger",
        "logging"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DJj123dj/logbucket.git"
    },
    "dependencies": {
        "ansis": "^3.17.0"
    },
    "devDependencies": {
        "@types/node": "^22.15.2"
    }
}
